<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.model.UserFoot">


	<insert id="JoinFoot" parameterType="com.smhrd.model.UserFoot">
		insert into foot values(
			#{user_id},
			#{foot_size},
			#{foot_width},
			#{foot_height},
			#{foot_flat}
			
		)
		
	</insert>



</mapper>